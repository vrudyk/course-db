<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solvd.dao.interfaces.IBanksDAO">
<resultMap id="bank" type="com.solvd.model.BankModel">
    <id property="idBank" column="idBank"/>
    <result property="Name" column="Name"/>
</resultMap>
<select id="getAllBankss" resultMap="bank">
    SELECT * FROM bank
</select>
    <insert id="addBank">
        INSERT INTO bank (idBank, Name) VALUES (#{idBank}, #{Name})
    </insert>
</mapper>