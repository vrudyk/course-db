<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solvd.dao.interfaces.IGradeBookDAO">
    <resultMap id="gradeBook" type="com.solvd.model.GradeBook">
        <id property="id" column="id"/>
        <result property="student_id" column="student_id"/>
        <result property="teacher_id" column="teacher_id"/>
        <result property="mark" column="mark"/>
        <result property="date" column="date"/>
        <result property="subject_id" column="subject_id"/>
    </resultMap>
    <select id="getAllGradeBooks" resultMap="gradeBook">
        SELECT * FROM `gradebook`
    </select>
</mapper>