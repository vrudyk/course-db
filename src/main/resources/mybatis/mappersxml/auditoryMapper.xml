<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.solvd.dao.interfaces.IAuditoryDAO">
    <resultMap id="auditory" type="com.solvd.model.Auditory">
        <id property="id" column="id"/>
        <result property="number" column="number"/>
        <result property="numberOfSeats" column="numberOfSeats"/>
    </resultMap>
    <select id="getAllAuditories" resultMap="auditory">
        SELECT * FROM `auditory`
    </select>
</mapper>